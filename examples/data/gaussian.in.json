{
"generator":{
    "start":{
        "type":"time",
        "params":{
            "tstart": {
                "value": 0, 
                "units": "sec"
                },
        }
    },
    "rand":{
        "type":"hammersley",
        "count":100000
    }
},
"beam": {"params": {"total_charge": {"value": 10, "units": "pC"}}}, 
"r_dist": {"type": "radial_gaussian", "params": {"sigma_xy":{"value":2,"units":"mm"}}}, 
"z_dist":{"type": "gaussian", "params": {"sigma_z":{"value":1,"units":"mm"}}},
"px_dist":{"type": "gaussian", "params": {"sigma_px":{"value":1,"units":"keV/c"}} },
"py_dist":{"type": "gaussian", "params": {"sigma_py":{"value":1,"units":"keV/c"}} },
"pz_dist":{"type": "gaussian", "params": {"sigma_pz":{"value":100,"units":"eV/c"}, "avg_pz":{"value":100,"units":"eV/c"}} },
"t_dist": {"type": "uniform", "params": {"min_t":{"value":-2,"units":"ps"},"max_t":{"value":2,"units":"ps"}}},
"output": {"type": "gpt", "file": "rad.gaussian.out.txt"}}
